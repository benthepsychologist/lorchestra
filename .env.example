# BigQuery Configuration for lorchestra Event Pipeline
# Copy this file to .env and fill in your actual values

# Google Cloud Project
GCP_PROJECT=lorchestra

# BigQuery Dataset (use events_dev for development, events_prod for production)
EVENTS_BQ_DATASET=events_dev

# BigQuery Tables
EVENT_LOG_TABLE=event_log
RAW_OBJECTS_TABLE=raw_objects

# Service Account Credentials
# Path to the JSON key file created during setup
GOOGLE_APPLICATION_CREDENTIALS=/path/to/lorchestra-events-key.json

# Optional: For debugging
# BQ_DEBUG=true
