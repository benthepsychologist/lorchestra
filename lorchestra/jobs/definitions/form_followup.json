{
  "job_id": "form_followup",
  "job_type": "final_form",
  "description": "Form measurement events from followup responses (GAD-7, PHQ-9, IPIP-NEO-60-C, FSCRS)",
  "source": {
    "filter": {
      "canonical_schema": "iglu:org.canonical/form_response/jsonschema/1-0-0",
      "connection_name": "google-forms-followup"
    }
  },
  "transform": {
    "binding_id": "followup",
    "binding_version": "1.0.0"
  },
  "sink": {
    "measurement_table": "measurement_events",
    "observation_table": "observations"
  },
  "events": {
    "on_started": "formation.started",
    "on_complete": "formation.completed",
    "on_fail": "formation.failed"
  }
}
